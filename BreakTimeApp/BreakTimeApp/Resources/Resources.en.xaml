<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <!--  App Info  -->
    <sys:String x:Key="app_name">BreakTimeApp</sys:String>

    <!--  MainWindow  -->
    <sys:Int32 x:Key="main_window_min_height">200</sys:Int32>
    <sys:Int32 x:Key="main_window_min_width">600</sys:Int32>
    <sys:String x:Key="main_window_notify_lbl">Notification interval</sys:String>
    <sys:String x:Key="main_window_btn">Confirm</sys:String>
    <sys:String x:Key="main_window_hour_lbl">hour</sys:String>
    <sys:String x:Key="main_window_minutes_lbl">minutes</sys:String>
    <sys:String x:Key="main_window_search">Search</sys:String>
    <sys:String x:Key="main_window_settings">Settings</sys:String>
    <sys:String x:Key="main_window_notify">Notify</sys:String>
    <sys:String x:Key="main_window_data">Data</sys:String>
    <sys:String x:Key="main_window_title">WPF UI - NotifyIcon2</sys:String>

    <!--  Tray  -->
    <sys:String x:Key="TrayHome">TrayHome</sys:String>

</ResourceDictionary>